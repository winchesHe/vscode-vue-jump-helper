{
  "publisher": "winches",
  "name": "<plugin-name>",
  "displayName": "<plugin-name>",
  "version": "1.0.0",
  "private": true,
  "description": "exchange selected words",
  "author": "winches",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/winchesHe/plugin-vscode-import-to-require.git"
  },
  "categories": [
    "Other"
  ],
  "icon": "res/logo.png",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [
    "onCommand:<plugin-name>"
  ],
  "contributes": {
    "commands": [
      {
        "command": "<plugin-name>",
        "title": "自动转换import 和 require"
      }
    ],
    "keybindings": [
      {
        "command": "<plugin-name>",
        "key": "ctrl+alt+R",
        "mac": "cmd+shift+R",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "<plugin-name>",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.0",
    "@antfu/ni": "^0.17.2",
    "@types/node": "^18.7.18",
    "@types/vscode": "^1.69.0",
    "eslint": "^8.46.0",
    "rimraf": "^3.0.2",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vite": "^3.0.4",
    "vitest": "^0.21.0",
    "vsce": "^2.10.0",
    "tsx": "^3.12.7",
    "lint-staged": "^13.1.2",
    "simple-git-hooks": "^2.8.1"
  }
}
