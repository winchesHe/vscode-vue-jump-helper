{
  "publisher": "winches",
  "name": "vue-jump-helper",
  "displayName": "vue-jump-helper",
  "version": "1.0.0",
  "private": true,
  "description": "辅助Vue中各函数的跳转",
  "author": "winches",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "categories": [
    "Other"
  ],
  "icon": "res/logo.png",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [
    "onLanguage:vue"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "vue-jump-helper 配置",
      "properties": {
        "vue-jump-helper.webpackConfigPath": {
          "type": "string",
          "description": "webpack 的配置文件路径，用来读取resolve中的alias，默认获取tsconfig"
        }
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.5",
    "@antfu/ni": "^0.17.2",
    "@types/node": "^18.19.3",
    "@types/vscode": "^1.69.0",
    "@vue/compiler-sfc": "^3.3.13",
    "acorn": "^8.10.0",
    "acorn-walk": "^8.2.0",
    "eslint": "^8.55.0",
    "lint-staged": "^13.1.2",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.9.0",
    "ts-morph": "^20.0.0",
    "tsup": "^7.3.0",
    "tsx": "^3.12.7",
    "typescript": "^5.3.3",
    "vite": "^3.0.4",
    "vitest": "^0.21.1",
    "vsce": "^2.15.0"
  }
}